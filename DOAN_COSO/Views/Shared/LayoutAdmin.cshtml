@{
    ViewBag.Title = "LayoutAdmin";
}
<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Dashboard V.3 | Nalika - Material Admin Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- favicon
        ============================================ -->
    <link rel="shortcut icon" type="image/x-icon" href="~/CssForAdmin/img/favicon.ico">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link href="~/CssForAdmin/css/accordions.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/CssForAdmin/css/bootstrap.min.css">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/font-awesome.min.css">
    <!-- nalika Icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/nalika-icon.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/owl.carousel.css">
    <link rel="stylesheet" href="~/CssForAdmin/css/owl.theme.css">
    <link rel="stylesheet" href="~/CssForAdmin/css/owl.transitions.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/normalize.css">
    <!-- meanmenu icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/meanmenu.min.css">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/main.css">
    <!-- morrisjs CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/morrisjs/morris.css">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- metisMenu CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/metisMenu/metisMenu.min.css">
    <link rel="stylesheet" href="~/CssForAdmin/css/metisMenu/metisMenu-vertical.css">
    <!-- calendar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/calendar/fullcalendar.min.css">
    <link rel="stylesheet" href="~/CssForAdmin/css/calendar/fullcalendar.print.min.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/style.css">

    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/CssForAdmin/css/responsive.css">
    <!-- modernizr JS
        ============================================ -->
    <script src="~/CssForAdmin/js/vendor/modernizr-2.8.3.min.js"></script>
    <link href="~/Content/toastr.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
</head>

<body>
    <div class="left-sidebar-pro">
        <nav id="sidebar" class="">
            <div class="sidebar-header">
                <a href="#"><img class="main-logo" src="~/CssForAdmin/img/logo/logo.png" alt="" /></a>
                <strong><img src="~/CssForAdmin/img/logo/logosn.png" alt="" /></strong>
            </div>
            <div class="collapse navbar-collapse">
                @Html.Partial("LgPartial")
            </div>

            <div class="left-custom-menu-adp-wrap comment-scrollbar">
                <nav class="sidebar-nav left-sidebar-menu-pro">
                    <ul class="metismenu" id="menu1">
                        <li class="active">
                            <a class="has-arrow" href="index.html">
                                <i class="icon nalika-home icon-wrap"></i>
                                <span class="mini-click-non">Quản Lý</span>
                            </a>
                            <ul class="submenu-angle" aria-expanded="true">
                                <li><a title="Product List" href="~/Admin/ThongKe"><span class="mini-sub-pro">Thống Kê</span></a></li>
                                <li><a title="Product Edit" href="~/Admin/ListLoaiSanPham"><span class="mini-sub-pro">Loại Sản Phẩm</span></a></li>
                                <li><a title="Product Detail" href="~/Admin/ListSanPham"><span class="mini-sub-pro">Sản phẩm</span></a></li>
                                <li><a title="Product Cart" href="~/Admin/ManageDonHang"><span class="mini-sub-pro">Quản Lý Đơn Hàng</span></a></li>
                                <li><a title="Product Payment" href="~/Role/Index"><span class="mini-sub-pro">Phân Quyền</span></a></li>
                            </ul>
                        </li>
                    </ul>
                </nav>
            </div>
        </nav>
    </div>
    <div class="all-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="index.html"><img class="main-logo" src="img/logo/logo.png" alt="" /></a>
                    </div>
                </div>
            </div>
        </div>
        @RenderBody()
    </div>

    <!-- Start Welcome area -->
    <!-- jquery
        ============================================ -->
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery-3.4.1.slim.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <!-- bootstrap JS
        ============================================ -->
    <script src="~/CssForAdmin/js/bootstrap.min.js"></script>
    <!-- wow JS
        ============================================ -->
    <script src="~/CssForAdmin/js/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="~/CssForAdmin/js/jquery-price-slider.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="~/CssForAdmin/js/jquery.meanmenu.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="~/CssForAdmin/js/owl.carousel.min.js"></script>
    <!-- sticky JS
        ============================================ -->
    <script src="~/CssForAdmin/js/jquery.sticky.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="~/CssForAdmin/js/jquery.scrollUp.min.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/CssForAdmin/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/CssForAdmin/js/scrollbar/mCustomScrollbar-active.js"></script>
    <!-- metisMenu JS
        ============================================ -->
    <script src="~/CssForAdmin/js/metisMenu/metisMenu.min.js"></script>
    <script src="~/CssForAdmin/js/metisMenu/metisMenu-active.js"></script>
    <!-- morrisjs JS
        ============================================ -->
    <script src="~/CssForAdmin/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/CssForAdmin/js/sparkline/jquery.charts-sparkline.js"></script>
    <!-- calendar JS
        ============================================ -->
    <script src="~/CssForAdmin/js/calendar/moment.min.js"></script>
    <script src="~/CssForAdmin/js/calendar/fullcalendar.min.js"></script>
    <script src="~/CssForAdmin/js/calendar/fullcalendar-active.js"></script>
    <!-- float JS
        ============================================ -->
    <script src="~/CssForAdmin/js/flot/jquery.flot.js"></script>
    <script src="~/CssForAdmin/js/flot/jquery.flot.resize.js"></script>
    <script src="~/CssForAdmin/js/flot/jquery.flot.pie.js"></script>
    <script src="~/CssForAdmin/js/flot/jquery.flot.tooltip.min.js"></script>
    <script src="~/CssForAdmin/js/flot/jquery.flot.orderBars.js"></script>
    <script src="~/CssForAdmin/js/flot/curvedLines.js"></script>
    <script src="~/CssForAdmin/js/flot/flot-active.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/CssForAdmin/js/plugins.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/CssForAdmin/js/main.js"></script>

    <script src="~/cssForCoffee/js/popper.min.js"></script>
    <script src="~/cssForCoffee/js/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("upload", required: false)
    @RenderSection("ChangeStatus", required: false)
    @RenderSection("loaisp", required: false)
    <script src="~/Scripts/toastr.js"></script>
    <script>
        $('#alertMessage').removeClass('hide');
        $('#alertMessage').delay(1000).slideUp(500);
    </script>
</body>
</html>



